	quest.0D159FD643581C18.title: "&f往昔的遗迹"
	quest.0D38D67E3FD120CB.quest_desc: [
		"[\"\", {\"text\": \"混沌\", \"color\": \"rainbow\"}, \"武器是\", {\"text\": \"神龙\", \"color\": \"gold\", \"underlined\": true, \"clickEvent\": { \"action\": \"change_page\", \"value\": \"5388B67410670702\" }, \"hoverEvent\": {\"action\": \"show_text\", \"contents\": \"前往神龙武器任务\"}}, \"武器的最终升级版，可安装\", {\"text\": \"升级模块\", \"color\": \"green\"}, \"的数量最多，拥有最强的可定制性。\"]"
